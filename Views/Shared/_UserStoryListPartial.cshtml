
@model List<WebApplication5.Models.UserStoryViewModel>

<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Description</th>
        <th>Owner</th>
    </tr>
    @foreach (var story in Model)
    {
        <tr>
            <td>@story.userStory.Id </td>
            <td>@story.userStory.Title</td>
            <td>@story.userStory.Description</td>
            <td>@story.Owner </td>
        </tr>
    }
</table>

