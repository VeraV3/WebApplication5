
@model List<WebApplication5.Models.UserStory>

<table>
    <tr>
        <th>ID</th>
        <th>Title</th>
        <th>Description</th>
        <th>Owner</th>
    </tr>
    @foreach (var story in Model)
    {
<tr>
    <td>@story.Id </td>
    <td>@story.Title</td>
    <td>@story.Description</td>
    @* <td>@story.Owner </td>*@
</tr>
    }
</table>

